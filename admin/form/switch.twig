{% set attributes = element.getAttributes() %}
{% set content = element.getContents() %}

<input {{ attributes|attr(['type'], true) }} type="hidden" value="0">
<div class="make-switch switch-large" tabindex="0" data-on-label="{{ content[1].getLabel() }}" data-off-label="{{ content[0].getLabel() }}">
	<input {{ attributes|attr(['type'], true) }} type="checkbox"{% if (element.getValue()|default(1)) == 1 %} checked{% endif %} value="1" id="inputActive">
</div>